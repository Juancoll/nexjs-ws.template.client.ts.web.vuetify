{
    "name": "tradin.web.home",
    "version": "0.0.1",
    "private": true,
    "description": "wsapi client demo",
    "author": "nexjs-cli",
    "scripts": {
        "serve": "vue-cli-service serve",
        "serve:production": "vue-cli-service serve --mode production",
        "serve:staging": "vue-cli-service serve --mode staging",
        "build:production": "vue-cli-service build --mode production",
        "build:staging": "vue-cli-service build --mode staging",
        "start": "serve dist",      
        "authorize": "vsts-npm-auth -config .npmrc",
        "wsclient:install": "npm link ../tradin.api.wsclient-ts && npm link @tradin/api.wsclient",
        "wsclient:uninstall": "npm unlink ../tradin.api.wsclient-ts && npm unlink @tradin/api.wsclient",
        "httpclient:install": "npm link ../tradin.api.httpclient-ts && npm link @tradin/api.httpclient",
        "httpclient:uninstall": "npm unlink ../tradin.api.httpclient-ts && npm unlink @tradin/api.httpclient",
        "install:local": "npm run wsclient:install && npm run httpclient:install",
        "uninstall:local": "npm run wsclient:uninstall && npm run httpclient:uninstall",
        "install:npm": "npm install @tradin/api.wsclient @tradin/api.httpclient --save",
        "uninstall:npm": "npm uninstall @tradin/api.wsclient @tradin/api.httpclient --save"
    },
    "dependencies": {
        "@mdi/font": "^4.7.95",
        "@types/lodash": "^4.14.149",
        "@types/socket.io-client": "^1.4.32",
        "core-js": "^3.6.2",
        "linq": "^3.2.1",
        "lodash": "^4.17.15",
        "markdown-it-katex": "^2.0.3",
        "markdown-it-vue": "^1.0.11",
        "moment": "^2.24.0",
        "roboto-fontface": "^0.10.0",
        "serve": "^11.3.0",
        "socket.io-client": "^2.3.0",
        "strongly-typed-events": "^1.6.4",
        "sweetalert2": "^9.5.4",
        "typescript-collections": "^1.3.3",
        "uuid": "^3.3.3",
        "vue": "^2.6.11",
        "vue-class-component": "^7.1.0",
        "vue-i18n": "^8.15.3",
        "vue-property-decorator": "^8.3.0",
        "vue-router": "^3.1.3",
        "vuetify": "^2.2.3",
        "vuex": "^3.1.2"
    },
    "devDependencies": {
        "@types/node" :"13.9.2",
        "@types/uuid": "^3.4.6",
        "@types/webpack": "^4.41.2",
        "@vue/cli-plugin-babel": "^4.1.2",
        "@vue/cli-plugin-typescript": "^4.1.2",
        "@vue/cli-service": "^4.1.2",
        "cross-env": "^6.0.3",
        "dotenv-cli": "^3.1.0",
        "html-loader": "^0.5.5",
        "node-sass": "^4.13.0",
        "sass": "^1.24.4",
        "sass-loader": "^8.0.0",
        "tslint": "^5.20.1",
        "typescript": "^3.7.4",
        "vue-cli-plugin-i18n": "^0.6.0",
        "vue-cli-plugin-pug": "^1.0.7",
        "vue-cli-plugin-vuetify": "^2.0.3",
        "vue-template-compiler": "^2.6.11",
        "vuetify-loader": "^1.4.3",
        "vuex-module-decorators": "^0.11.0"
    }
}